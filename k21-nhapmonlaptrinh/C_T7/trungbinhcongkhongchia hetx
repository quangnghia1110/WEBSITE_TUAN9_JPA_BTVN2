#include<stdio.h>
#include <stdlib.h>
#include <math.h>
#include <string.h>

int main()
{
    int a[100], n, x;
    scanf("%d", &n);
    scanf("%d", &x);
    int sum = 0;
    int count = 0;
    int i;
    for (i = 0; i < n; ++i) {
        scanf("%d", &a[i]);
    }
    for (int i = 0; i < n; i++) {
        if (a[i] % x != 0) {
            sum += a[i];
            count++;
        }
    }
    int b = int(sum / count);
        printf("%d", b);
    return 0;
}
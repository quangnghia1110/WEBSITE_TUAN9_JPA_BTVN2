#include <stdio.h>
int maiin()
{
	int a[5],i,j,dem=0;
	for(i=0;i<5;i++)
	do{
		printf("Moi ban nhap gia tri cua phan tu mang a[%d]",i);
		scanf("%d",&a[i]);
	}while(a[i]<=0);
	for(i=0;i<5;i++)
	for(j=2;j<a[i];j++)
	{	if (a[i]%j==0)
		printf("%d khong phai la so nguyen to",a[i]);
		return 0;
		dem++;
	}
	if(dem==0)
		printf("mang nay la so nguyen to");
	
}
